{% extends 'layoutForm.twig' %}
{% block title %}
  <div class="header-irac">
    <a  href="/SIA/juridico/turnos/{{ id }}">Nueva Asignacion</a>
    <a href="#!" class="active">Respuestas</a>
  </div>
{% endblock %}

  {% block btnHeader %}

      <select class="form-control" id="personal-turnado" data-id="{{ id }}">
        <option value="">Escoga Personal</option>
        {% for turno in turnados %}
        <option value="{{turno.idPuestoJuridico}}">{{turno.saludo}} {{turno.nombre}} {{turno.paterno}} {{turno.materno}}</option>
        {% endfor %}
      </select>
{% endblock %}


{% block form %}
    
    <form id="irac"   data-ruta="Irac">
      
    </form>
   <div class="alert alert-danger" id="errors">
     
   </div>
{% endblock %}
